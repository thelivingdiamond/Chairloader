<?xml version="1.0"?>
<MergingPolicy
    xmlns="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader ../../../../../Xsd/Chairloader/MergingPolicy.xsd"
    fileNameRegex=".*\.xml">
    <Node name="ArkAiTree">
        <Attributes />
        <Collection type="nodeDict" />

        <ChildNodes>
            <Node name="Root">
                <Attributes />
                <Collection type="nodeDict" />

                <ChildNodes>
                    <Node name="ArkAiTreeStackNode">
                        <Attributes />
                        <Collection type="nodeDict" />

                        <ChildNodes>
                            <Node name="Children" allowAnyChildrenInXsd="true">
                                <Attributes />
        
                                <Patches>
                                    <AddChildIndex name="ch:index" />
                                </Patches>
                        
                                <Collection type="array">
                                    <ChildIndexAttribute name="ch:index" />
                                </Collection>

                                <ChildNodes>
                                    <Node name=".*" nameRegex="true" allowAnyChildrenInXsd="true">
                                        <Attributes allowUnknown="true">
                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                        </Attributes>

                                        <XsdAliases />
                                        
                                        <Collection type="nodeDict" />

                                        <ChildNodes>
                                            <!-- ArkAiTreeLurkNode -->
                                            <Node name="LurkAttackContextIds">
                                                <Attributes />
                        
                                                <Patches>
                                                    <AddChildIndex name="ch:index" />
                                                </Patches>
                                        
                                                <Collection type="array">
                                                    <ChildIndexAttribute name="ch:index" />
                                                </Collection>

                                                <ChildNodes>
                                                    <Node name="Element">
                                                        <Attributes allowUnknown="true">
                                                            <Attribute name="value" type="int32" required="true" readOnly="true" />
                                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                                        </Attributes>
                                                    </Node>
                                                </ChildNodes>
                                            </Node>
                                            
                                            <!-- ArkAiTreeCorruptNpcNode -->
                                            <Node name="CorruptNpcCombatAbilityContextProfileIds">
                                                <Attributes />
                        
                                                <Patches>
                                                    <AddChildIndex name="ch:index" />
                                                </Patches>
                                        
                                                <Collection type="array">
                                                    <ChildIndexAttribute name="ch:index" />
                                                </Collection>

                                                <ChildNodes>
                                                    <Node name="Element">
                                                        <Attributes allowUnknown="true">
                                                            <Attribute name="value" type="int32" required="true" readOnly="true" />
                                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                                        </Attributes>
                                                    </Node>
                                                </ChildNodes>
                                            </Node>
                                            
                                            <!-- ArkAiTreeCreatePhantomNode -->
                                            <Node name="PossiblePhantomArchetypeNames">
                                                <Attributes />
                        
                                                <Patches>
                                                    <AddChildIndex name="ch:index" />
                                                </Patches>
                                        
                                                <Collection type="array">
                                                    <ChildIndexAttribute name="ch:index" />
                                                </Collection>

                                                <ChildNodes>
                                                    <Node name="Element">
                                                        <Attributes allowUnknown="true">
                                                            <Attribute name="value" type="ArchetypeName" required="true" readOnly="true" />
                                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                                        </Attributes>
                                                    </Node>
                                                </ChildNodes>
                                            </Node>
                                            
                                            <!-- ArkAiTreeCreatePhantomNode -->
                                            <Node name="CorrespondingRaisePhantomContextIds">
                                                <Attributes />
                        
                                                <Patches>
                                                    <AddChildIndex name="ch:index" />
                                                </Patches>
                                        
                                                <Collection type="array">
                                                    <ChildIndexAttribute name="ch:index" />
                                                </Collection>

                                                <ChildNodes>
                                                    <Node name="Element">
                                                        <Attributes allowUnknown="true">
                                                            <Attribute name="value" type="int32" required="true" readOnly="true" />
                                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                                        </Attributes>
                                                    </Node>
                                                </ChildNodes>
                                            </Node>
                                            
                                            <!-- ArkAiTreeCreatePhantomNode -->
                                            <Node name="NoMovementRaisePhantomContextIds">
                                                <Attributes />
                        
                                                <Patches>
                                                    <AddChildIndex name="ch:index" />
                                                </Patches>
                                        
                                                <Collection type="array">
                                                    <ChildIndexAttribute name="ch:index" />
                                                </Collection>

                                                <ChildNodes>
                                                    <Node name="Element">
                                                        <Attributes allowUnknown="true">
                                                            <Attribute name="value" type="int32" required="true" readOnly="true" />
                                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                                        </Attributes>
                                                    </Node>
                                                </ChildNodes>
                                            </Node>

                                            <!-- Allow text type because Ark/AI/AiTrees/NightmareAiTree.xml -->
                                            <!-- has an error in ArkAiTreeNightmareCombatNode node -->
                                            <Node name=".*" nameRegex="true" recursive="true" textType="string" allowEmptyText="true" allowAnyChildrenInXsd="true" >
                                                <Attributes allowUnknown="true" />
                                                <XsdAliases />
                                                <Collection type="nodeDict" />
                                            </Node>
                                        </ChildNodes>
                                    </Node>
                                </ChildNodes>
                            </Node>
                        </ChildNodes>
                    </Node>
                </ChildNodes>
            </Node>
        </ChildNodes>
    </Node>
</MergingPolicy>

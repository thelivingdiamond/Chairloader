<?xml version="1.0"?>
<MergingPolicy
    xmlns="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader ../../../../Xsd/Chairloader/MergingPolicy.xsd">
    <Node name="NpcPrecacheList">
        <Attributes />

        <Collection type="nodeDict" />

        <ChildNodes>
            <Node name="SignalReceivers">
                <Attributes />
        
                <Collection type="dict">
                    <PrimaryKey>
                        <ChildAttribute name="file" />
                    </PrimaryKey>
                </Collection>

                <ChildNodes>
                    <Node name="SignalReceiver">
                        <Attributes>
                            <Attribute name="file" type="string" required="true" readOnly="true" />
                        </Attributes>
                    </Node>
                </ChildNodes>
            </Node>
            
            <Node name="SignalAccumulators">
                <Attributes />
        
                <Collection type="dict">
                    <PrimaryKey>
                        <ChildAttribute name="file" />
                    </PrimaryKey>
                </Collection>

                <ChildNodes>
                    <Node name="SignalAccumulator">
                        <Attributes>
                            <Attribute name="file" type="string" required="true" readOnly="true" />
                        </Attributes>
                    </Node>
                </ChildNodes>
            </Node>
            
            <Node name="AiTrees">
                <Attributes />
        
                <Collection type="dict">
                    <PrimaryKey>
                        <ChildAttribute name="file" />
                    </PrimaryKey>
                </Collection>

                <ChildNodes>
                    <Node name="AiTree">
                        <Attributes>
                            <Attribute name="file" type="string" required="true" readOnly="true" />
                        </Attributes>
                    </Node>
                </ChildNodes>
            </Node>
        </ChildNodes>
    </Node>
</MergingPolicy>

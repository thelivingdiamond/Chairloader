<?xml version="1.0"?>
<MergingPolicy
    xmlns="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader ../../../Xsd/Chairloader/MergingPolicy.xsd">
    <Node name="WorldUIInterferenceConfig">
        <Attributes />
        <Collection type="nodeDict" />

        <ChildNodes>
            <Node name="StandardInterference">
                <Attributes />

                <Collection type="dict">
                    <PrimaryKey>
                        <ChildAttribute name="paramName" />
                    </PrimaryKey>
                </Collection>

                <ChildNodes>
                    <Node name="MaterialParam">
                        <Attributes>
                            <Attribute name="paramName" type="string" required="true" readOnly="true" />
                            <Attribute name="paramValue" type="float" required="true" />
                        </Attributes>
                    </Node>
                </ChildNodes>
            </Node>

            <Node name="TranscribeDisruption">
                <Attributes />

                <Collection type="dict">
                    <PrimaryKey>
                        <ChildAttribute name="paramName" />
                    </PrimaryKey>
                </Collection>

                <ChildNodes>
                    <Node name="MaterialParam">
                        <Attributes>
                            <Attribute name="paramName" type="string" required="true" readOnly="true" />
                            <Attribute name="paramValue" type="float" required="true" />
                        </Attributes>
                    </Node>
                </ChildNodes>
            </Node>
        </ChildNodes>
    </Node>
</MergingPolicy>

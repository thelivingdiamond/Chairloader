<?xml version="1.0"?>
<MergingPolicy
    xmlns="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader ../../../../../Xsd/Chairloader/MergingPolicy.xsd">
    <Node name="FontConfig">
        <Attributes />
        <Collection type="nodeDict" />

        <ChildNodes>
            <Node name=".*" nameRegex="true">
                <Attributes>
                    <Attribute name="fontlib" type="string" required="true" />
                </Attributes>

                <Collection type="nodeDict" />

                <ChildNodes>
                    <Node name=".*" nameRegex="true" textType="string">
                        <Attributes>
                            <Attribute name="scale" type="float" />
                            <Attribute name="y" type="float" />
                        </Attributes>
                    </Node>
                </ChildNodes>
            </Node>
        </ChildNodes>
    </Node>
</MergingPolicy>

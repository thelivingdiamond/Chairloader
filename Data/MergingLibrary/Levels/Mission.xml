                                            <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                                        </Attributes>
                                    </Node>
                                </ChildNodes>
                            </Node>
                        </ChildNodes>
                    </Node>
                </ChildNodes>

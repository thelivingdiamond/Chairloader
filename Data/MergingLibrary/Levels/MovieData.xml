<?xml version="1.0"?>
<MergingPolicy
    xmlns="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader ../../../Xsd/Chairloader/MergingPolicy.xsd"
    fileNameRegex=".*/moviedata\.xml"
    recursive="true">
    <Node name="MovieData">
        <Attributes />

        <Collection type="nodeDict" />

        <ChildNodes>
            <Node name="Mission">
                <Attributes>
                    <Attribute name="Name" type="string" required="true" />
                </Attributes>
                
                <Collection type="nodeDict" />

                <ChildNodes>
                    <Node name="SequenceData">
                        <Attributes />
        
                        <Collection type="dict">
                            <PrimaryKey>
                                <ChildAttribute name="Name" />
                            </PrimaryKey>
                        </Collection>
        
                        <ChildNodes>
                            <Node name="Sequence">
                                <Attributes>
                                    <Attribute name="Name" type="string" required="true" readOnly="true" />
                                    <Attribute name="SequenceVersion" type="int32" required="true" />
                                    <Attribute name="Flags" type="int32" required="true" />
                                    <Attribute name="StartTime" type="float" required="true" />
                                    <Attribute name="EndTime" type="float" required="true" />
                                    <Attribute name="ID" type="int32" required="true" />
                                    <Attribute name="ConversationID" type="string" required="true" />
                                </Attributes>
                                
                                <Collection type="nodeDict" />
        
                                <ChildNodes>
                                    <Node name="Nodes">
                                        <Attributes />
        
                                        <Collection type="dict">
                                            <PrimaryKey>
                                                <ChildAttribute name="Name" />
                                            </PrimaryKey>
                                        </Collection>
        
                                        <ChildNodes>
                                            <Node name="Node">
                                                <Attributes allowUnknown="true">
                                                    <Attribute name="Id" type="int32" required="true" />
                                                    <Attribute name="Type" type="string" required="true" />
                                                    <Attribute name="Name" type="string" required="true" readOnly="true" />
                                                    <Attribute name="Flags" type="int32" required="true" />
                                                    <Attribute name="ParentNode" type="int32" />
                                                    <Attribute name="paramIdVersion" type="int32" required="true" />
                                                </Attributes>
        
                                                <Collection type="replaceOnly" />
        
                                                <ChildNodes>
                                                    <Node name="Track">
                                                        <Attributes allowUnknown="true">
                                                            <Attribute name="paramUserValue" type="string" />
                                                            <Attribute name="paramType" type="string" required="true" />
                                                            <Attribute name="defaultValue" type="string" />
                                                            <Attribute name="Type" type="int32" required="true" />
                                                            <Attribute name="AnimationLayer" type="int32" />
                                                            <Attribute name="Voice" type="VoiceId" />
                                                            <Attribute name="PAChannel" type="int32" />
                                                            <Attribute name="Flags" type="int32" required="true" />
                                                            <Attribute name="StartTime" type="float" />
                                                            <Attribute name="EndTime" type="float" />
                                                            <Attribute name="HasCustomColor" type="boolInt" required="true" />
                                                            <Attribute name="ValueType" type="int32" required="true" />
                                                        </Attributes>
        
                                                        <Collection type="replaceOnly" />
        
                                                        <ChildNodes>
                                                            <Node name="Key">
                                                                <Attributes allowUnknown="true">
                                                                    <Attribute name="time" type="float" required="true" />
                                                                </Attributes>
                                                            </Node>
        
                                                            <Node name="NewSubTrack">
                                                                <Attributes>
                                                                    <Attribute name="Flags" type="int32" required="true" />
                                                                    <Attribute name="defaultValue" type="string" required="true" />
                                                                    <Attribute name="HasCustomColor" type="boolInt" required="true" />
                                                                </Attributes>
        
                                                                <Collection type="replaceOnly" />
        
                                                                <ChildNodes>
                                                                    <Node name="Key">
                                                                        <Attributes allowUnknown="true">
                                                                            <Attribute name="time" type="float" required="true" />
                                                                        </Attributes>
                                                                    </Node>
                                                                </ChildNodes>
                                                            </Node>
                                                        </ChildNodes>
                                                    </Node>
                                                </ChildNodes>
                                            </Node>
                                        </ChildNodes>
                                    </Node>

                                    <Node name="Events">
                                        <Attributes />

                                        <Collection type="dict">
                                            <PrimaryKey>
                                                <ChildAttribute name="Name" />
                                            </PrimaryKey>
                                        </Collection>

                                        <ChildNodes>
                                            <Node name="Event">
                                                <Attributes>
                                                    <Attribute name="Name" type="string" required="true" readOnly="true" />
                                                </Attributes>
                                            </Node>
                                        </ChildNodes>
                                    </Node>
                                </ChildNodes>
                            </Node>
                        </ChildNodes>
                    </Node>
                </ChildNodes>
            </Node>
        </ChildNodes>
    </Node>
</MergingPolicy>

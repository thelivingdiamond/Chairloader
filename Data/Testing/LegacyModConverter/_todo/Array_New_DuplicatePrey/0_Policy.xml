<?xml version="1.0"?>
<MergingPolicy
    xmlns="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://thelivingdiamond.github.io/Chairloader/Xsd/Chairloader ../../../../Xsd/Chairloader/MergingPolicy.xsd">
    <Node name="Root">
        <Attributes />

        <Patches>
            <AddChildIndex name="ch:index" />
        </Patches>

        <Collection type="array">
            <ChildIndexAttribute name="ch:index" />
        </Collection>

        <ChildNodes>
            <Node name="Item">
                <Attributes>
                    <Attribute name="attr1" type="string" />
                    <Attribute name="attr2" type="string" />
                    <Attribute name="ch:index" type="int32" required="true" readOnly="true" generated="true" />
                </Attributes>
            </Node>
        </ChildNodes>
    </Node>
</MergingPolicy>

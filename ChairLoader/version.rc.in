
//// Product Version
//#define _VERSION 0,0,1,0
//#define _VERSION_STR "0.0.1"

// Naming
#define _PRODUCTNAME "Chairloader Library"
#define _COMPANYNAME "thelivingdiamond"
#define _FILEDESCRIPTION "A modding framework for prey (2017)"
#define _INTERNALNAME "Chairloader"
#define _LEGALCOPYRIGHT "Copyright (C) 2022 thelivingdiamond"
#define _ORIGINALFILENAME "Chairloader.dll"


1 VERSIONINFO
 FILEVERSION @_CHAIRLOADER_VERSION_MAJOR@.@_CHAIRLOADER_VERSION_MINOR@.@_CHAIRLOADER_VERSION_PATCH@.@_CHAIRLOADER_VERSION_BUILD@
 PRODUCTVERSION @_CHAIRLOADER_VERSION_MAJOR@.@_CHAIRLOADER_VERSION_MINOR@.@_CHAIRLOADER_VERSION_PATCH@.@_CHAIRLOADER_VERSION_BUILD@
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x00000002L
#endif
 FILEOS 0x4L
 FILETYPE 0x00000002L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "Comments", _PRODUCTNAME
            VALUE "CompanyName", _COMPANYNAME
            VALUE "FileDescription", _FILEDESCRIPTION
            VALUE "FileVersion", "@_CHAIRLOADER_VERSION_MAJOR@.@_CHAIRLOADER_VERSION_MINOR@.@_CHAIRLOADER_VERSION_PATCH@@_CHAIRLOADER_VERSION_TYPE@"
            VALUE "InternalName", _INTERNALNAME
            VALUE "LegalCopyright", _LEGALCOPYRIGHT
            VALUE "OriginalFilename", _ORIGINALFILENAME
            VALUE "ProductName", _PRODUCTNAME
            VALUE "ProductVersion", "@_CHAIRLOADER_VERSION_MAJOR@.@_CHAIRLOADER_VERSION_MINOR@.@_CHAIRLOADER_VERSION_PATCH@@_CHAIRLOADER_VERSION_TYPE@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
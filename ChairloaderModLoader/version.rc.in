// File Version
#define _FILEVERSION 1,0,1,0
#define _FILEVERSION_STR "1.0.1.0-Beta"

// Product Version
#define _PRODUCTVERSION 1,0,1,0
#define _PRODUCTVERSION_STR "1.0.1.0-Beta"

// Naming
#define _PRODUCTNAME "Chairloader Mod Loader"
#define _COMPANYNAME "thelivingdiamond"
#define _FILEDESCRIPTION "A mod loader for prey (2017) and the Chairloader framework"
#define _INTERNALNAME "ChairloaderModLoader"
#define _LEGALCOPYRIGHT "Copyright (C) 2022 thelivingdiamond"
#define _ORIGINALFILENAME "ChairloaderModLoader.exe"


1 VERSIONINFO
 FILEVERSION _FILEVERSION
 PRODUCTVERSION _PRODUCTVERSION
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x00000002L
#endif
 FILEOS 0x4L
 FILETYPE 0x00000001L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "Comments", _PRODUCTNAME
            VALUE "CompanyName", _COMPANYNAME
            VALUE "FileDescription", _FILEDESCRIPTION
            VALUE "FileVersion", _FILEVERSION_STR
            VALUE "InternalName", _INTERNALNAME
            VALUE "LegalCopyright", _LEGALCOPYRIGHT
            VALUE "OriginalFilename", _ORIGINALFILENAME
            VALUE "ProductName", _PRODUCTNAME
            VALUE "ProductVersion", _PRODUCTVERSION_STR
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
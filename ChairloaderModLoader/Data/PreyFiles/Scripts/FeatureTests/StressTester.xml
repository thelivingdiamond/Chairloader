 <Tests setName="repeatThese">
  <FeatureTest name="EachWeaponForAWhile" enabled="true" require="localPlayerExists" iterateOverParams="Revolver;Hammer;Gauss;DSG1;Grendel;Nova;Jackal;K-Volt;SCAR;Feline;SCARAB;AY69;Mk60;mike;LTag;FragGrenades;FlashBangGrenades" maxTime="3600">
   <TrySpawnPlayer who="localPlayer"/>
   <WaitUntilPlayerIsAlive localPlayer="true"/>
   <SetItem className="%1"/>
   <Wait duration="6"/>
   <TrySpawnPlayer who="localPlayer"/>
   <WaitUntilPlayerIsAlive localPlayer="true"/>
   <MovePlayerToOtherEntity localPlayer="true" className="DummyPlayer"/>
   <SetItem className="%1"/>
   <Wait duration="6"/>
   <DoConsoleCommand command="kill"/>
   <Wait duration="5"/>
  </FeatureTest>
  <FeatureTest name="Restart" enabled="true">
   <DoConsoleCommand command="g_testStatus 0"/>
   <DoConsoleCommand command="ft_runall repeatThese"/>
  </FeatureTest>
 </Tests>
 <Tests setName="repeatTheseShort">

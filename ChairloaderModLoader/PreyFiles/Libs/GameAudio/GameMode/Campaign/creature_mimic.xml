
<ATLConfig atl_name="Creature_Mimic">
 <AudioTriggers>
  <ATLTrigger atl_name="Play_Mimic_Hide_LP" path="Mimic_Mvmt">
   <WwiseEvent wwise_name="Play_Mimic_Hide_LP"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Stop_Mimic_Hide_LP" path="Mimic_Mvmt">
   <WwiseEvent wwise_name="Stop_Mimic_Hide_LP"/>
  </ATLTrigger>
 </AudioTriggers>
 <AudioPreloads>
  <ATLPreloadRequest atl_name="Load_Mimic" atl_type="AutoLoad">
   <ATLPlatforms>
    <Platform atl_name="pc" atl_config_group_name="default"/>
    <Platform atl_name="ps4" atl_config_group_name="default"/>
    <Platform atl_name="xboxone" atl_config_group_name="default"/>
    <Platform atl_name="mac" atl_config_group_name="default"/>
    <Platform atl_name="linux" atl_config_group_name="default"/>
   </ATLPlatforms>
   <ATLConfigGroup atl_name="default">
    <WwiseFile wwise_name="Mimic.bnk"/>
   </ATLConfigGroup>
  </ATLPreloadRequest>
 </AudioPreloads>
</ATLConfig>
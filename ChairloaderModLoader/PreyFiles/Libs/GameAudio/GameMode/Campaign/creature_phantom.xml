
<ATLConfig atl_name="Creature_Phantom">
 <AudioTriggers>
  <ATLTrigger atl_name="Play_Phantom_Hypnotize_In" path="FX">
   <WwiseEvent wwise_name="Play_Phantom_Hypnotize_In"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_Phantom_Hypnotize_Swish" path="FX">
   <WwiseEvent wwise_name="Play_Phantom_Hypnotize_Swish"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_Phantom_Lift_Impact" path="FX">
   <WwiseEvent wwise_name="Play_Phantom_Lift_Impact"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_Phantom_Vox_Lift" path="Vox">
   <WwiseEvent wwise_name="Play_Phantom_Vox_Lift"/>
  </ATLTrigger>
 </AudioTriggers>
 <AudioPreloads>
  <ATLPreloadRequest atl_name="Load_Phantom" atl_type="AutoLoad">
   <ATLPlatforms>
    <Platform atl_name="pc" atl_config_group_name="default"/>
    <Platform atl_name="ps4" atl_config_group_name="default"/>
    <Platform atl_name="xboxone" atl_config_group_name="default"/>
    <Platform atl_name="mac" atl_config_group_name="default"/>
    <Platform atl_name="linux" atl_config_group_name="default"/>
   </ATLPlatforms>
   <ATLConfigGroup atl_name="default">
    <WwiseFile wwise_name="Phantom.bnk"/>
   </ATLConfigGroup>
  </ATLPreloadRequest>
 </AudioPreloads>
</ATLConfig>
                    <attribute name="name" priority="1"/>
                    <nodeStructure>
                        <_wildcard_>
                            <_wildcard_ merge_children="true"/>
                        </_wildcard_>
                    </nodeStructure>
                </mergingPolicy>
            </ArkWeaponMods.xml>
            <Config.xml>
                <mergingPolicy identification_policy="match_attribute">
                    <attribute name="name" priority="0"/>
                    <nodeStructure>
                       <WeaponConfig>
                           <Weapons merge_children="true"/>
                           <Stats merge_children="true"/>
                       </WeaponConfig>
                    </nodeStructure>

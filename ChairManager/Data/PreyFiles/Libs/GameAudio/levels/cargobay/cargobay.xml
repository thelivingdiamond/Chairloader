
<ATLConfig atl_name="CargoBay">
 <AudioTriggers>
  <ATLTrigger atl_name="Play_CargoBay_Hydraulic_Cargo_Init" path="Objects">
   <WwiseEvent wwise_name="Play_CargoBay_Hydraulic_Cargo_Init"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Stop_CargoBay_Hydraulic_Cargo_FloorContact" path="Objects">
   <WwiseEvent wwise_name="Stop_CargoBay_Hydraulic_Cargo_FloorContact"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Stop_CargoBay_Hydraulic_Cargo_Raised" path="Objects">
   <WwiseEvent wwise_name="Stop_CargoBay_Hydraulic_Cargo_Raised"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Stop_CargoWinch_LP" path="Objects">
   <WwiseEvent wwise_name="Stop_CargoWinch_LP"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_CargoWinch_LP" path="Objects">
   <WwiseEvent wwise_name="Play_CargoWinch_LP"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_CargoBay_Restore_Power" path="Objects">
   <WwiseEvent wwise_name="Play_CargoBay_Restore_Power"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Play_ChainFence_Move_LP" path="Objects">
   <WwiseEvent wwise_name="Play_ChainFence_Move_LP"/>
  </ATLTrigger>
  <ATLTrigger atl_name="Stop_ChainFence_Move_LP" path="Objects">
   <WwiseEvent wwise_name="Stop_ChainFence_Move_LP"/>
  </ATLTrigger>
 </AudioTriggers>
 <AudioPreloads>
  <ATLPreloadRequest atl_name="CargoBay.bnk" atl_type="AutoLoad">
   <ATLPlatforms>
    <Platform atl_name="pc" atl_config_group_name="default"/>
    <Platform atl_name="ps4" atl_config_group_name="default"/>
    <Platform atl_name="xboxone" atl_config_group_name="default"/>
    <Platform atl_name="mac" atl_config_group_name="default"/>
    <Platform atl_name="linux" atl_config_group_name="default"/>
   </ATLPlatforms>
   <ATLConfigGroup atl_name="default">
    <WwiseFile wwise_name="CargoBay.bnk"/>
   </ATLConfigGroup>
  </ATLPreloadRequest>
 </AudioPreloads>
</ATLConfig>
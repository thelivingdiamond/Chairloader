
<UIElements name="DanielleLoadingScreen">
 <UIElement name="DanielleLoadingScreen" render_lockless="1">
  <GFx file="GFX/danielle_loading.gfx" layer="30">
   <Constraints>
    <Align mode="dynamic" valign="center" halign="center" scale="1" max="0"/>
   </Constraints>
  </GFx>
  <functions>
   <function name="loadingStart" funcname="load.loadingStart"/>
   <function name="setText" funcname="load.setText">
    <param name="ZoneName" desc="defaults to TALOS 1" type="string"/>
    <param name="tip" type="string"/>
    <param name="Image" desc="savegame images for now" type="string"/>
   </function>
   <function name="loadingProgress" funcname="load.loadingProgress">
    <param name="progress" desc="0-1 percent loaded" type="float"/>
   </function>
   <function name="loadingStop" funcname="load.loadingStop"/>
   <function name="screenRotate" funcname="camera.rotateOffset">
    <param name="xValue" desc="-1 to +1" type="float"/>
    <param name="yValue" desc="-1 to +1" type="float"/>
   </function>
  </functions>
  <Events/>
  <Arrays/>
  <variables/>
 </UIElement>
</UIElements>